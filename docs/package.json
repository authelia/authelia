    "@docsearch/js": "3.9.0",
    "@tabler/icons": "3.37.1",
    "@thulite/doks-core": "1.9.0",
    "@thulite/images": "3.3.4",
    "@thulite/inline-svg": "1.2.2",
    "@thulite/seo": "2.4.3",
    "bootstrap-icons": "1.13.1",
    "exec-bin": "1.0.0",
    "hugo-installer": "4.0.1",
    "thulite": "2.6.4"
  },
  "devDependencies": {
    "vite": "7.3.1"
  },
  "engines": {
    "node": ">=22.14.0",
    "pnpm": "10"
  },
  "packageManager": "pnpm@10.6.4",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "pnpm": {
    "overrides": {
